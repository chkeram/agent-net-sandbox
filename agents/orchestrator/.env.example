# Multi-Protocol Agent Orchestrator Environment Configuration

# Application Settings
APP_NAME="Multi-Protocol Agent Orchestrator"
APP_VERSION="0.1.0"
DEBUG=false
ENVIRONMENT=development

# Server Configuration
HOST=0.0.0.0
PORT=8000
WORKERS=1

# LLM Provider Configuration
LLM_PROVIDER=both  # Options: openai, anthropic, both
OPENAI_API_KEY=sk-your-openai-api-key-here
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here
DEFAULT_MODEL_TEMPERATURE=0.7

# OpenAI Settings
OPENAI_MODEL=gpt-4o
OPENAI_MAX_TOKENS=4096
OPENAI_TIMEOUT=30.0

# Anthropic Settings
ANTHROPIC_MODEL=claude-3-5-sonnet-20240620
ANTHROPIC_MAX_TOKENS=4096
ANTHROPIC_TIMEOUT=30.0

# Discovery Settings
DISCOVERY_INTERVAL_SECONDS=30
DISCOVERY_TIMEOUT_SECONDS=5
DOCKER_NETWORK=agent-network
DOCKER_SOCKET_PATH=/var/run/docker.sock

# Routing Settings
ROUTING_TIMEOUT_SECONDS=30.0
MAX_RETRIES=3
RETRY_DELAY_SECONDS=1.0
ENABLE_FALLBACK_ROUTING=true

# Monitoring and Metrics
ENABLE_METRICS=true
METRICS_PORT=9090
LOG_LEVEL=INFO
STRUCTURED_LOGGING=true

# Security
CORS_ORIGINS=["*"]
CORS_METHODS=["GET", "POST", "PUT", "DELETE"]
CORS_HEADERS=["*"]